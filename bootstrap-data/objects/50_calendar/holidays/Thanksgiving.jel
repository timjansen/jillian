Thing('Thanksgiving', @HolidayCategory,
	properties={
    month: 11,
    firstDate: LocalDate(1942, 11, 26),
 		isMatch: (this, date)=>with thanksgiving=this.matchesInYear(date.year).first: date == thanksgiving,
		matchesInYear: (this, year)=>if year >= this.firstDate.year then [LocalDate(year, this.month).allDays.nthMatch(4, d=>d.dayOfWeek == 4)] else []
  }
)
