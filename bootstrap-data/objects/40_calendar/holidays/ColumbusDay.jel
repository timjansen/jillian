Thing('MartinLutherKindJrDay', @HolidayCategory,
	properties={
    month: 10,
    firstDate: LocalDate(1492, 10, 12),
 		isMatch: (this, date)=>with cday=this.matchesInYear(this, date.year).first: date == cday,
		matchesInYear: (this, year)=>year >= this.firstDate.year ? LocalDate(year, this.month).allDays.nthMatch(2, d=>d.dayOfWeek == 1) : []
  }
)
