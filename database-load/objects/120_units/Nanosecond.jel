Thing('Nanosecond', @UnitOfMeasureCategory,
	facts=FactHelper.createRootFacts({
		quantityCategory: @Time,
		isPrimaryUnit: false,
		type: UnitTypeEnum.METRIC,
		idealMax: 1000,
		convertsTo: {
			Second: {factor: Fraction(1, 1e9)},
			Millisecond: {factor: Fraction(1, 1e6)},
			Microsecond: {factor: Fraction(1, 1e3)}
		}
}))


