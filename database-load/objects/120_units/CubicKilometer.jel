Thing('CubicKilometer', @UnitOfMeasureCategory,
	facts=FactHelper.createRootFacts({
		quantityCategory: @Volume,
		isPrimaryUnit: false,
		type: UnitTypeEnum.METRIC,
		idealMin: 1,
		convertsTo: {
			CubicMeter: {factor: 1e6}
		},
		createFrom: [Unit({Kilometer: 3}), Unit({Kilometer: 1, SquareKilometer: 1})]
}))


