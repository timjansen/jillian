Thing('Turn', @UnitOfMeasureCategory,
	properties={
		quantityCategory: @Angle,
		isPrimaryUnit: false,
		type: UnitTypeEnum.OTHER,
		idealMin: 0.25,
		convertsTo: {
			Degree: {f: v=>v*360, factor: 360},
			Radian: {f: v=>v*2*Math.PI, factor: 2*Math.PI},
			Gradian: {f: v=>v*400, factor: 400}
		},
		usedBy: [@RotationsPerMinute]
})


