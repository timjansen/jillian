Thing('Centimeter', @UnitOfMeasureCategory,
	facts=FactHelper.createRootFacts({
		quantityCategory: @Length,
		isPrimaryUnit: false,
		type: UnitTypeEnum.SI,
		idealMin: 1,
		idealMax: 100,
		convertsTo: {
			Meter: {factor: Fraction(1, 100)}
		},
		usedBy: [@CubicCentimeter, @SquareCentimeter]
}))


