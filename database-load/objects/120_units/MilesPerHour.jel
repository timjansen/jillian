Thing('MilesPerHour', @UnitOfMeasureCategory,
	facts=FactHelper.createRootFacts({
		quantityCategory: @Velocity,
		isPrimaryUnit: false,
		type: UnitTypeEnum.IMPERIAL,
		convertsTo: {
			MeterPerSecond: {factor: 0.44704},
			KilometerPerHour: {factor: 1.609344}
		},
		createFrom: [Unit({Mile: 1, Hour: -1})]
		
}))


