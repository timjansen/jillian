Thing('Microsecond', @UnitOfMeasureCategory,
	facts=FactHelper.createRootFacts({
		quantityCategory: @Time,
		isPrimaryUnit: false,
		type: UnitTypeEnum.METRIC,
		idealMin: 1,
		idealMax: 1000,
		convertsTo: {
			Second: {factor: Fraction(1, 1e6)},
			Millisecond: {factor: Fraction(1, 1000)},
			Nanosecond: {factor: 1e3}
		}
}))


