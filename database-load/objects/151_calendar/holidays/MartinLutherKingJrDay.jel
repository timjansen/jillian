Thing('MartinLutherKingJrDay', @HolidayCategory,
	facts=FactHelper.createRootFacts({
    month: 1,
    firstDate: LocalDate(1986, 1, 18),
 		isMatch: (date)=>let mlkday=this.matchesInYear(date.year).first: date == mlkday,
		matchesInYear: (year)=>if year >= this.firstDate.year then [LocalDate(year, this.month).allDays.nthMatch(3, d=>d.dayOfWeek == 1)] else []
  }
))
